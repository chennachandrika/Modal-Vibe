# Azure OpenAI Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Azure OpenAI API Key (get from Azure Portal)
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here

# Azure OpenAI Base URL (e.g., https://your-resource-name.openai.azure.com)
AZURE_OPENAI_API_BASE=your-azure-openai-base-url-here

# Azure OpenAI API Version
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# Azure OpenAI Deployment Name
AZURE_OPENAI_DEPLOYMENT=gpt-4o

# Optional: Admin Secret for admin endpoints
# ADMIN_SECRET=your-admin-secret-here
